{
  "manifest_version": 2,
  "name": "Safe Browser",
  "version": "1.0",

  "description": "Block url that can't access by children and organize by their parents",

  "icons": {
    "48": "icon/Logo_Purple_Logo.svg",
    "96": "icon/Logo_Purple_Logo.svg"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["blocker.js"]
    }
  ],

  "permissions": ["*://safebrowser.herokuapp.com/history*", "webRequest"]
}
